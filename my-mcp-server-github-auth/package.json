{
	"name": "remote-mcp-github-oauth",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"build": "echo 'No build step required for MCP server'",
		"deploy": "wrangler deploy",
		"deploy:full": "./deploy.sh",
		"deploy:db": "wrangler d1 execute symbolai-financial-db --file=./schema.sql",
		"deploy:secrets": "echo 'Set secrets: GITHUB_CLIENT_ID, GITHUB_CLIENT_SECRET, COOKIE_ENCRYPTION_KEY'",
		"dev": "wrangler dev",
		"start": "wrangler dev",
		"cf-typegen": "wrangler types",
		"type-check": "tsc --noEmit",
		"db:list": "wrangler d1 list",
		"db:query": "wrangler d1 execute symbolai-financial-db",
		"secrets:list": "wrangler secret list"
	},
	"dependencies": {
		"@cloudflare/workers-oauth-provider": "^0.1.0",
		"@modelcontextprotocol/sdk": "1.22.0",
		"agents": "^0.2.19",
		"hono": "^4.10.6",
		"just-pick": "^4.2.0",
		"octokit": "^5.0.4",
		"workers-mcp": "^0.0.13",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"@types/node": "^24.10.0",
		"prettier": "^3.6.2",
		"typescript": "5.9.3",
		"wrangler": "^4.50.0"
	}
}